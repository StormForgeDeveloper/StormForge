        </Command>
        
        <Command Name="RequestUGCZoneInstance" Reliable="true" Crypto="No" Trace="Debug1" Desc="Request ugc zone instance">
            <Cmd>
                <Params TypeName="uint64" Name="UGCID" Desc="UGC content id"/>
            </Cmd>
            <Res>
                <Params TypeName="VariableTable" Name="GameInstance" Desc="Privisoned UGC zone instance information"/>
            </Res>
        </Command>


        <!-- ===================================================  Shop & Item ================================================== -->

		<Command Name="BuyShopItemPrepare" Reliable="true" Crypto="No" Trace="Debug1" Desc="Buy shop item prepare">
			<Cmd>
				<Params TypeName="uint32" Name="ShopItemID" Desc="Shop table id"/>
			</Cmd>
			<Res>
				<Params TypeName="uint32" Name="ShopItemID" Desc="Shop table id"/>
				<Params TypeName="String" Name="PurchaseID" Desc="Purchase ID"/>
